}

Question 6.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		if(i%2==0)
		printf("%d\n",i);
	}

}

Question 7.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	for(i=n;i>=1;i--)
	{
		if(i%2==0)
		printf("%d\n",i);
	}
}

Question 8.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	printf("square of %d is : %d\n",i,i*i);
}

Question 9.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	printf("cube of %d is : %d\n",i,i*i*i);
}

Question 10.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	
	for(i=1;i<=10;i++)
	printf("tab of %d is : %d\n",n,n*i);
}
