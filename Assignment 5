Question 1.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	printf("MySirG\n");
}

Question 2.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	printf("%d\n",i);
}

Question 3.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	for(i=n;i>=1;i--)
	printf("%d\n",i);
}

Question 4.
#include<stdio.h>
int main()
{
	int n,i;
	printf("Enter a no:");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		if(i&1)
		printf("%d\n",i);
	}

}

